
<div>
  <h1>Productos y Servicios</h1>
  <ul class="productos">
    {{#each productos}}
    <li class="producto">
      <img src="{{imagen}}" alt="{{nombre}}">
      <h2>{{nombre}}</h2>
      <p>{{descripcion}}</p>
      <span class="precio">${{precio}}</span>
    </li>
    {{/each}}
  </ul>

  <div class="paginacion">
    {{#if previousPage}}
    <a href="/products?page={{previousPage}}">Página anterior</a>
    {{/if}}

    {{#if nextPage}}
    <a href="/products?page={{nextPage}}">Página siguiente</a>
    {{/if}}
  </div>
</div>

