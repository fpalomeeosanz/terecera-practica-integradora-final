<div>
<h1>Listado de Productos</h1>
<ul>
  {{#each products}}
    <li>
      <h2>{{this.name}}</h2>
      <p>{{this.description}}</p>
      <a href="/products/{{this._id}}">Ver detalles</a>
    </li>
  {{/each}}
</ul>

  {{#if pagination}}
    <ul class="pagination">
      <li><a href="?page={{pagination.prevPage}}">Anterior</a></li>
      {{#each pagination.pages}}
        <li><a href="?page={{this}}"> {{this}} </a></li>
      {{/each}}
      <li><a href="?page={{pagination.nextPage}}">Siguiente</a></li>
    </ul>
  {{/if}}
</div>
<hr>
<a href="/"><button>Salir</button></a>
<a href="/current"><button>Ir a mi session</button></a>

